                                        <option value="4">Película preferida</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Ingrese su respuesta</label>
                                    <input type="text" class="form-control" id="respuesta_recu" placeholder="Ingrese su respuesta">
                                </div>



                            </form>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
