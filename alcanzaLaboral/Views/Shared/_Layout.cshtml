    <!-- Modal Registro -->
    <div class="modal fade" id="registro_mod" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Crear una Cuenta</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <form>
                                <div class="form-group">
                                    <label>Nombre de usuario</label>
                                    <input type="text" class="form-control" id="usu_reg" aria-describedby="emailHelp" placeholder="Ingrese un usuario" onblur="ValidarUsuario()">
                                </div>
                                <div class="form-group">
                                    <label>Contraseña</label>
                                    <input type="password" class="form-control" id="pass_reg" placeholder="Ingrese su contraseña">
                                </div>
                                <div class="form-group">
                                    <label>Repita su contraseña</label>
                                    <input type="password" class="form-control" id="rpass_reg" placeholder="Repita su contraseña">
                                </div>
                                <div class="form-group">
                                    <label>E-mail de recepción de ofertas</label>
                                    <input type="text" class="form-control" id="email_reg" placeholder="Ingrese su e-mail" onblur="ValidarCorreo()">
                                </div>
                                <div class="form-group">
                                    <label>Coloque nuevamente su e-mail</label>
                                    <input type="text" class="form-control" id="remail_reg" placeholder="Repita su e-mail">
                                </div>
                                <div class="form-group">
                                    <label>Seleccione una pregunta secreta</label>
                                    <select class="form-control" id="preg_reg">
                                        <option value="1">Apellido materno de la madre</option>
                                        <option value="2">Ciudad de nacimiento</option>
                                        <option value="3">Nombre de la primera mascota</option>
                                        <option value="4">Película preferida</option>
                                    </select>

                                </div>
                                <div class="form-group">
                                    <label>Respuesta</label>
                                    <input type="text" class="form-control" id="respuesta_reg" placeholder="Ingrese su respuesta">
                                </div>
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" id="chk_terminos">
                                    <label class="custom-control-label" for="chk_terminos">Acepto los <a href="../Inicio/TerminosCondiciones" target="_blank">Términos y Condiciones</a></label>
                                </div>


                            </form>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="genric-btn primary-border" data-dismiss="modal">Cerrar</button>
                    <button id="registrarCandidatosbtn" type="button" class="genric-btn primary" onclick="Registrar_Onclick()">Guardar</button>
                </div>
            </div>
        </div>
    </div>

