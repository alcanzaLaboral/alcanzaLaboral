    <link href="~/Content/Layout/nice-select.css" rel="stylesheet" />
    <link href="~/Content/Layout/animate.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/Layout/main.css" rel="stylesheet" />
    <link href="~/Content/PerfilProfesional.css" rel="stylesheet" />
</head>
<body style="background-color: #ccc;">

    <div id="container-floating">

        <div class="nd4 nds" data-toggle="tooltip" data-placement="left" title="Regresar" style="text-align:center;" onclick="regresar()">
            <i style="color:#fff;margin-top: 0.75rem;" class="fas fa-fast-backward"></i>
        </div>
        <div class="nd3 nds" data-toggle="tooltip" data-placement="left" title="Contactar candidato"  style="text-align:center;" onclick="contactar()">
            <i style="color:#fff;margin-top: 0.75rem;" class="fas fa-user-plus"></i>
        </div>
        <div class="nd1 nds" data-toggle="tooltip" data-placement="left" title="Imprimir"  style="text-align:center;" onclick="fprint()">
            <i style="color:#fff;margin-top: 0.75rem;" class="fas fa-print"></i>
        </div>

        <div id="floating-button" data-toggle="tooltip" data-placement="left" data-original-title="Create">
            <p class="plus">+</p>
            <img class="edit" src="https://ssl.gstatic.com/bt/C3341AA7A1A076756462EE2E5CD71C11/1x/bt_compose2_1x.png">
        </div>



    </div>

    <div class="container" style="background-color: #fff;">
        <br>
        <br>
        <div id="datosgenerales_tab">
            <div class="row" style="">
                <div class="col-md-8" style="text-align: center;">
                    <h3 id="nombre_id"></h3>
                    <div style="text-align: left; width: 39%; margin: auto; padding: 10px;">
                        <span style="font-weight: 500;">Fecha Nacimiento:</span>&nbsp;<span id="fechanaci_id"></span><br>
                        <span style="font-weight: 500;">Edad:</span>&nbsp;<span id="edad_id"></span><br>
                        <span style="font-weight: 500;">País de Nacionalidad:</span>&nbsp;<span id="nacionalidad_id"></span><br>
                        <span style="font-weight: 500;">Residencia:</span>&nbsp;<span id="residencia_id"></span><br>
                        <span style="font-weight: 500; display:none;">Teléfono:</span>&nbsp;<span style="display:none;" id="fono_id"></span><br>
                        <span style="font-weight: 500; display:none;">Correo:</span>&nbsp;<span style="display:none;" id="correo_id"></span><br>
                    </div>

                </div>
                <div class="col-md-4" style="padding-top: 20px;">
                    <div id="mostrarfoto_perfil">

                    </div>

                </div>

            </div>
        </div>
        <div id="perfilpro_tab">
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h5>Perfil Profesional:</h5>
                <p id="resumenpro_id">

                </p>
            </div>
        </div>
        <div id="CatePro_tab">
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Datos Categoría Profesional:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col">CATEGORIA PROFESIONAL</th>
                            <th scope="col">ESPECIALIDAD</th>
                        </tr>
                    </thead>
                    <tbody id="bodyCategoria"></tbody>
                </table>

            </div>
        </div>
        <div id="TipoEstudio_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Tipos de Estudio:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col">TIPO</th>
                            <th scope="col">NOMBRE</th>
                            <th scope="col">CULMINADO</th>
                        </tr>
                    </thead>
                    <tbody id="bodyInstitucion"></tbody>
                </table>


            </div>
        </div>
        <div id="ExperienciaLaboral_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Experiencia Laboral:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col">DESCRIPCION</th>
                            <th scope="col">TITULO/CARGO</th>
                            <th scope="col">EMPRESA</th>
                        </tr>
                    </thead>
                    <tbody id="bodyExperiencia"></tbody>
                </table>


            </div>
        </div>
        <div id="AreaIntereses_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Areas de Interés:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col" style="text-align: center;">ÁREA O PUESTO</th>
                        </tr>
                    </thead>
                    <tbody id="bodyInteres"></tbody>
                </table>


            </div>
        </div>
        <div id="DomSoft_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Dominio de Software:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col" style="text-align: center;">PROGRAMA</th>
                            <th scope="col" style="text-align: center;">NIVEL</th>
                        </tr>
                    </thead>
                    <tbody id="bodyDSoftware"></tbody>
                </table>


            </div>
        </div>
        <div id="EquipoMaq_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Equipo / Maquinaria:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col" style="text-align: center;">EQUIPO/MAQUINARIA</th>
                            <th scope="col" style="text-align: center;">NIVEL</th>
                        </tr>
                    </thead>
                    <tbody id="bodyMaquinaria"></tbody>
                </table>


            </div>
        </div>
        <div id="Idioma_tab">
            <div style="height: 15px;"></div>
            <div style="width: 90%;border-bottom: 3px solid #373737; margin: auto;"></div>
            <div style="width: 90%; margin: auto; padding-left: 15px;padding-right: 15px; padding-top: 17px;">
                <h4>Dominio de Idiomas:</h4>
                <br>
                <table class="table table-sm" style="width: 90%; margin: auto;">
                    <thead>
                        <tr>
                            <th scope="col" style="text-align: center;">IDIOMA</th>
                            <th scope="col" style="text-align: center;">HABLADO</th>
                            <th scope="col" style="text-align: center;">ESCRITO</th>
                            <th scope="col" style="text-align: center;">TRADUCCIÓN</th>
                        </tr>
                    </thead>
                    <tbody id="bodyIdioma"></tbody>
                </table>


            </div>
        </div>




        <br>
        <br>
    </div>

    <input style="display:none;" id="btnContactar" type="button" value="contactar" data-toggle="modal" data-target="#contac_mod"/>

    <!-- Modal Contactarse -->
    <div class="modal fade" id="contac_mod" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Contactar Candidato</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <form>
                                <div class="form-group">
                                    <label>Ingrese Nombre de Persona que desea contactar al candidato(a):</label>
                                    <input type="text" class="form-control" id="nom_contacto">
                                </div>
                                <div class="form-group">
                                    <label>Ingrese su email:</label>
                                    <input type="text" class="form-control" id="email_contacto" onblur="validarEmail(this)">
                                </div>
                                <div class="form-group">
                                    <label>Teléfono de Contacto:</label>
                                    <input type="text" class="form-control" id="fono_contacto" onkeypress="return justNumbers(event)">
                                </div>
                                <div class="form-group">
                                    <label>Descripción del Puesto: (40 Caracteres)</label>
                                    <textarea class="form-control" id="desc_contacto" rows="2" maxlength="40"></textarea>
                                </div>
                                <label id="nombredestinatario" style="display:none;"></label>
                                <label id="correodestinatario" style="display:none;"></label>

                            </form>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button id="cancelarContactar" type="button" class="genric-btn primary-border" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="genric-btn primary" onclick="Contactar_Onclick()">Enviar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- End footer Area -->

    <script src="~/Scripts/Layout/vendor/jquery-2.2.4.min.js"></script>
    <script src="~/Scripts/Layout/popper.min.js"></script>
    <script src="~/Scripts/Layout/vendor/bootstrap.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhOdIF3Y9382fqJYt5I_sswSrEw5eihAA"></script>
    <script src="~/Scripts/Layout/jquery-ui.js"></script>
    <script src="~/Scripts/Layout/easing.min.js"></script>
    <script src="~/Scripts/Layout/hoverIntent.js"></script>
    <script src="~/Scripts/Layout/superfish.min.js"></script>
    <script src="~/Scripts/Layout/jquery.ajaxchimp.min.js"></script>
    <script src="~/Scripts/Layout/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/Layout/jquery.nice-select.min.js"></script>
    <script src="~/Scripts/Layout/owl.carousel.min.js"></script>
    <script src="~/Scripts/Layout/mail-script.js"></script>
    <script src="~/Scripts/Layout/main.js"></script>      
    <script src="~/Scripts/icons.js"></script>

    <script src="~/Scripts/Modulo/General.js"></script>
    <script src="~/Scripts/Modulo/PerfilProfesional.js"></script>

    <script>

        function fprint() {
            window.print();
        }

        function contactar() {
            nombre = document.getElementById('nombre_id').innerHTML;
            correo = document.getElementById('correo_id').innerHTML;
            
            
            $('#btnContactar').click();
        }

        function regresar() {
            
            if (sessionStorage.getItem('peticionver') == 1) {
                window.location = "../Inicio/BuscarCandidatos";
                sessionStorage.setItem('peticionver', null);
            } else {
                window.location = "../Inicio/EditarPerfil";
                sessionStorage.setItem('peticionver', null);
            }
            

        }

    </script>

</body>
</html>
