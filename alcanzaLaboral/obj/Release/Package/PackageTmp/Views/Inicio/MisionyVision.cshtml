
@{
    ViewBag.Title = "MisionyVision";
}

<!-- start banner Area -->
<section class="about-banner relative">
    <div class="overlay overlay-bg"></div>
    <div class="container">
        <div class="row d-flex align-items-center justify-content-center">
            <div class="about-content col-lg-12">
                <h1 class="text-white">
                    Alcanza Laboral
                </h1>
                <p class="text-white link-nav"><a href="index.html">Inicio </a> &nbsp; <i class="fa fa-arrow-right"></i> &nbsp; <a href="insurance.html"> Misión y Visión</a></p>
            </div>
        </div>
    </div>
</section>
<!-- End banner Area -->
<!-- Start insurence-one Area -->
<section class="insurence-one-area section-gap">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 insurence-left">
                <img class="img-fluid img-one" src="~/img/mision.jpg" alt="mision" />
            </div>
            <div class="col-lg-6 insurence-right">
                <h6 class="text-uppercase">Alcanza Laboral</h6>
                <h1>Misión</h1>
                <p align="justify">
                    Ser un medio que facilite conseguir puestos de trabajos dignos, estables y adecuados, que sean acordes al conocimiento, experiencia y habilidad de toda clase de persona.
                </p>

            </div>
        </div>
    </div>
</section>
<!-- End insurence-one Area -->
<!-- Start insurence-two Area -->
<section class="insurence-two-area pb-120">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 insurence-left">
                <h6 class="text-uppercase">Alcanza Laboral</h6>
                <h1>Visión</h1>
                <p align="justify">
                    Ser el principal directorio laboral de ayuda empresarial y personnal del Perú, donde se contacten empresas y candidatos, que contribuya al bienestar personal de las personas y a la reducción de problemas sociales y económicos del país.
                </p>

            </div>
            <div class="col-lg-6 insurence-right">
                <img class="img-fluid img-two" src="~/img/vision.jpg" alt="vision" />
            </div>
        </div>
    </div>
</section>
<!-- End insurence-two Area -->

<script src="~/Scripts/Modulo/Inicio.js"></script>
